<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.senasoft_1.registrovehiculo.Fragmentos.FragmentInformes">

    <!-- TODO: Update blank fragment layout -->

    <Button
        android:id="@+id/btnEntradas"
        android:layout_width="170sp"
        android:layout_height="wrap_content"
        android:text="Entradas"
        android:background="@color/Black"
        android:textColor="@color/White"
        android:layout_marginTop="5sp"
        android:layout_marginLeft="5sp"/>

    <Button
        android:id="@+id/btnSalidas"
        android:layout_width="170sp"
        android:layout_height="wrap_content"
        android:text="Salidas"
        android:layout_gravity="right"
        android:background="@color/Black"
        android:textColor="@color/White"
        android:layout_marginRight="5sp"
        android:layout_marginTop="5sp"/>

    <ListView
        android:id="@+id/listaRegistros"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="80sp"></ListView>

</FrameLayout>
